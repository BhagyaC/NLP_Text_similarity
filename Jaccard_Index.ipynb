   "source": [
    "def Jaccard_index(sent1,sent2):\n",
    "    intersection = set(sent1).intersection(set(sent2))\n",
    "    union = set(sent1).union(set(sent2))\n",
    "    return len(intersection)/len(union)"
   ]
